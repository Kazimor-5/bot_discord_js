{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prefer-const": 2,
    "no-var": 2,
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
